{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "updater:default",
    "updater:allow-check",
    "updater:allow-download-and-install",
    "dialog:allow-ask",
    "process:allow-exit",
    "process:allow-restart",
    "os:default",
    "os:allow-platform",
    "oauth:allow-start"
  ]
}
